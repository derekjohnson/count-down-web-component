{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "airbnb-base"
  ],
  "rules": {
    "class-methods-use-this": [
      "error",
      {
        "exceptMethods": [
          "_createCountdownItem",
          "_getTimeRemaining"
        ]
      }
    ],
    "object-curly-newline": [
      "error",
      {
        "ObjectExpression": "always",
        "ObjectPattern": "never"
      }
    ]
  }
}